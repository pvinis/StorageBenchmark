{
  "name": "storagebenchmark",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "doctor": "bun x expo-doctor@latest",
    "doctor:fix": "expo install --check",
    "prebuild": "bun x expo prebuild",
    "gen:charts": "bun x -p @mermaid-js/mermaid-cli mmdc -i charts.md -e png; mv charts.md-1.png images/chart-device-release.png; mv charts.md-2.png images/chart-device-debug.png; mv charts.md-3.png images/chart-simulator-debug.png"
  },
  "dependencies": {
    "@op-engineering/op-sqlite": "^11.2.13",
    "@react-native-async-storage/async-storage": "1.23.1",
    "expo": "~52.0.25",
    "expo-dev-client": "~5.0.9",
    "expo-router": "~4.0.16",
    "expo-secure-store": "^14.0.1",
    "expo-sqlite": "~15.0.6",
    "lodash": "^4.17.21",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-keychain": "^9.2.2",
    "react-native-mmkv": "^3.2.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "realm": "^12.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.14",
    "@types/react": "~18.3.12",
    "typescript": "^5.3.3"
  }
}
